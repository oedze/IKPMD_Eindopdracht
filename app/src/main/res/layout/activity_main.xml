<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ga naar edit test"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="178dp" />

    <Button
        android:id="@+id/button_main_gotoround"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="GA NAAR RONDE"
        app:layout_constraintTop_toBottomOf="@+id/button_main_gotologin"
        tools:layout_editor_absoluteX="66dp" />

    <Button
        android:id="@+id/button_main_gotostep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="GA NAAR STAP"
        app:layout_constraintTop_toBottomOf="@+id/button_main_gotologin"
        tools:layout_editor_absoluteX="65dp" />

    <Button
        android:id="@+id/button_main_gotoregister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ga naar registreeer"
        app:layout_constraintTop_toBottomOf="@+id/button"
        tools:layout_editor_absoluteX="65dp" />

    <Button
        android:id="@+id/button_main_gotologin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ga naar login"
        app:layout_constraintTop_toBottomOf="@+id/button_main_gotoregister"
        tools:layout_editor_absoluteX="66dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button_main_export_data"
        android:text="Export"
        app:layout_constraintTop_toBottomOf="@id/button_main_gotostep"
        tools:layout_editor_absoluteX="66dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>